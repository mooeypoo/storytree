    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "^29.7.0",
    "rollup": "^4.12.0",
    "ts-jest": "^29.1.2",
